{
  "private": true,
  "packageManager": "pnpm@8.6.10",
  "scripts": {
    "build": "turbo build",
    "clean": "turbo clean",
    "dev": "dotenv -- turbo dev --no-cache  --continue --concurrency=11",
    "basic": "dotenv -- turbo dev --no-cache  --continue --concurrency=11 --filter=@feel-auth/web --filter=api",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo lint",
    "test": "turbo test",
    "typecheck": "turbo typecheck"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "turbo": "latest"
  },
  "dependencies": {
    "@types/express-serve-static-core": "^4.17.36"
  }
}
